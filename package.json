{
  "name": "osu-pulse-client",
  "version": "0.9.0",
  "packageManager": "yarn@1.22.0",
  "description": "OSU Music player",
  "author": {
    "name": "T1MON"
  },
  "license": "AGPL-3.0",
  "scripts": {
    "web:dev": "cross-env VITE_PLATFORM=web vite --open --host --port 8080",
    "web:build": "cross-env VITE_PLATFORM=web vite build",
    "electron:dev": "cross-env VITE_PLATFORM=electron concurrently --kill-others \"vite --host --port 3000\" \"tsc electron/main.ts electron/preload.ts && electron electron/main.js\"",
    "electron:build": "cross-env VITE_PLATFORM=electron vite build && tsc electron/main.ts electron/preload.ts && electron-builder --config --win",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@vue/apollo-composable": "^4.0.0-alpha.18",
    "@vueuse/core": "^9.2.0",
    "axios": "^1.3.3",
    "bootstrap-icons-vue": "^1.8.1",
    "color": "^4.2.3",
    "color-thief-ts": "^1.1.1",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.11.2",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "socket.io-client": "^4.5.4",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-spinner": "^1.0.4",
    "yandex-metrika-vue3": "^1.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@types/color": "^3.0.3",
    "@types/node": "^18.11.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/test-utils": "^2.0.0-rc.21",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.18.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "vite": "^4.1.1",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.28.5"
  }
}
